{
    "name": "akmeh/api-transformer",
    "description": "API Tool for transform the output easily between Laravel/Lumen API.",
    "keywords": ["framework", "laravel", "api"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.1.13",
        "league/fractal": "^0.17.0"
    },
    "autoload": {
         "psr-4": {
            "APITransformer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
